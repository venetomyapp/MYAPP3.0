{
  "name": "myapp31",
  "version": "3.0.0",
  "description": "MyApp - Progressive Web App per gestione richieste e servizi",
  "main": "index.html",
  "homepage": "https://myapp31.vercel.app",
  "type": "module",
  "scripts": {
    "dev": "npx http-server . -p 3000 -c-1",
    "start": "npm run dev",
    "build": "echo 'Build completato - App pronta per deploy'",
    "deploy": "git add . && git commit -m 'Deploy update' && git push origin main",
    "test": "echo 'Test webhook: curl -X POST https://myapp31.vercel.app/api/jotform-webhook'",
    "serve": "npx http-server . -p 8080",
    "webhook-test": "node test/webhook-test.js"
  },
  "keywords": [
    "pwa",
    "myapp",
    "webapp",
    "mobile-app",
    "supabase",
    "tailwind",
    "jotform",
    "webhook",
    "richieste"
  ],
  "author": {
    "name": "Antonio Grande",
    "email": "antoniogrande81@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniogrande81/la-mia-app2026.git"
  },
  "bugs": {
    "url": "https://github.com/antoniogrande81/la-mia-app2026/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "pwa": {
    "name": "MyApp",
    "short_name": "MyApp",
    "description": "Sistema integrato di gestione richieste e servizi",
    "theme_color": "#1a1a2e",
    "background_color": "#F4F4F8",
    "display": "standalone",
    "orientation": "portrait"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "vercel": {
    "functions": {
      "api/jotform-webhook.js": {
        "runtime": "nodejs18.x",
        "memory": 512
      }
    }
  }
}
